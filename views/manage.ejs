<%- include('layouts/header',{title: "Pet Manager"}) %>
<body>
  <table class="table table-striped table-dark">
    <tbody>
      <% for (let e of pets){ %>
        <tr>
          <td><%= e.id %></td>
          <td><%= e.name %></td>
          <td><%= e.foto %></td>
          <td><%= e.email %></td>
          <td><%= e.created_at %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>